use "assert"
use "math"

bool fn is_prime?(const int n) {
  if n < 2 return false
  every int i in 2..(Math->isqrt(n) + 1)
    if n % i == 0 return false
  true
}

bool fn prime_in_range?(const Range range) {
  every int x in range
    if is_prime?(x) return true
  false
}

Assert->truthy(prime_in_range?(10..15)) ## true
Assert->falsey(prime_in_range?(62..66)) ## false
Assert->truthy(prime_in_range?(3..5)) ## true
