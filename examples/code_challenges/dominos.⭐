use "bc_challenges"

string fn domino(string sequence) {
  mut string result = ""
  mut bool finished = false

  every char ch in sequence {
    if ch == '|' and not finished
      result += '/'
    else {
      if ch == ' ' or ch == '/'
        finished = true
      result += ch
    }
  }

  result
}

BeginnerCodes::test(664, domino)