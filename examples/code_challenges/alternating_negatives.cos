use "beginner_codes"

bool fn alternating_negatives?(int[] nums) {
  if nums.includes?(0)
    return false

  every int i in 0..(#nums - 2) {
    if (nums[i] >= 0 and nums[i + 1] >= 0) or (nums[i] < 0 and nums[i + 1] < 0)
      return false
  }

  true
}

BeginnerCodes::test(666, alternating_negatives?)