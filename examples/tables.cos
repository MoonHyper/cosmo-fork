type Person = string->string

Person fn new_person(string name, string address) {
  {
    name -> name,
    address -> address
  }
}

const Person bobert = new_person("bobert", "123 bobert st")
puts(bobert.name) ## . is valid
puts(bobert->address) ## -> is also valid

const string->string->string a = {
  b -> {
    c -> "hello world"
  }
}

puts(a::b->c) ## even :: is valid