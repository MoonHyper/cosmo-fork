const string->(string|int) bobert = {{
  name -> "bobert",
  address -> "123 bobert st",
  age -> 69
}}

puts(bobert.name) ## . is valid
puts(bobert::address) ## :: is also valid
puts(bobert->age)

string->string->string a = {{
  b -> {{
    c -> "hello world"
  }}
}}

puts(a->b->c) ## even -> is valid
a->b->foo = "bar"
puts(a->b->foo) ## bar