int[] fn simple_pair(int[] vec, int n) {
  int i = 0
  every int x in vec {
    int y = vec[i + 1]
    int z = vec[i - 1]
    if y == none {
      next
    }
    if x * y == n {
      return [x, y]
    }
    if x * z == n {
      return [x, z]
    }
    i += 1
  }
}

puts(simple_pair([1, 2, 3], 3)) ## [1, 3]
puts(simple_pair([1, 2, 3], 6)) ## [2, 3]
puts(simple_pair([1, 2, 3], 9)) ## none