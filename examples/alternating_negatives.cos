use "assert"

bool fn alternating_negatives?(int[] nums) {
  mut bool negative
  every int n in nums {
    if not (negative is void) and (n < 0 and negative or n >= 0 and not negative)
      return false
    negative = n < 0
  }
  true
}

Assert->truthy(alternating_negatives?([3, -2, 5, -5, 2, -8]))
Assert->truthy(alternating_negatives?([-6, 1, -1, 4, -3]))
Assert->falsey(alternating_negatives?([4, 4, -2, 3, -6, 10]))
